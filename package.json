{
  "name": "j-Arc",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prettier:fix": "prettier --write 'sandbox/**/*.java'"
  },
  "private": true,
  "devDependencies": {
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
